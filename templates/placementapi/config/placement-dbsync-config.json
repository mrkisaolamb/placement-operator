{
    "command": "placement-manage db sync",
    "config_files": [
        {
            "source": "/var/lib/config-data/placement.conf",
            "dest": "/etc/placement/placement.conf",
            "owner": "placement",
            "perm": "0600"
        },
        {
            "source": "/var/lib/config-data/custom.conf",
            "dest": "/etc/placement/placement.conf.d/custom.conf",
            "owner": "placement",
            "perm": "0600"
        }
    ]
}
